{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "resourceGroupName": {
      "value": "${AZURE_RESOURCEGROUP_NAME}"
    },
    "keyVaultName": {
      "value": "${AZURE_KEYVAULT_NAME}"
    },
    "storageAccountName": {
      "value": "${AZURE_STORAGEACCT_NAME}"
    },
    "docIntelligenceAccountName": {
      "value": "${AZURE_DOCUMENTINTELLIGENCE_ACCOUNT_NAME}"
    },
    "aiSearchName": {
      "value": "${AZURE_AISEARCH_NAME}"
    },
    "currentUserObjectId": {
      "value": "${AZURE_CURRENT_USER_OBJECT_ID}"
    },
    "aiFoundryName": {
      "value": "${AZURE_AIFOUNDRY_NAME}"
    },
    "appInsightsName": {
      "value": "${AZURE_APPINSIGHTS_NAME}"
    }
  }
}